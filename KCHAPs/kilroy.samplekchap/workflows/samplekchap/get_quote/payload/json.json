                                "type": "json",
                                "arg": {
                                    "type": "variable",
                                    "arg": "webhook_args"
                                }
                            }
                        }
                    },
                    {
                        "operation": "set_var_to",
                        "isServer": true,
                        "args": {
                            "var_name": "ticker",
                            "push_array_name": null,
                            "field_name": null,
                            "var_expr": {
                                "type": "object",
