                    {
                        "operation": "set_var_to",
                        "isServer": true,
                        "args": {
                            "var_name": "msg_quote",
                            "push_array_name": null,
                            "field_name": null,
                            "var_expr": {
                                "type": "results",
                                "arg": "__results__"
                            }
                        }
                    },
                    {
                        "operation": "set_var_to",
                        "isServer": true,
                        "args": {
