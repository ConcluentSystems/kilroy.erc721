                        }
                    },
                    {
                        "operation": "set_value",
                        "isServer": true,
                        "args": {
                            "variable": "msg",
                            "value": "${ticker} Bid: ${bid} at ${ts}"
                        }
                    },
                    {
                        "operation": "set_var_to",
                        "isServer": true,
                        "args": {
                            "var_name": "msg_quote",
                            "push_array_name": null,
                            "field_name": null,
                            "var_expr": {
                                "type": "variable",
                                "arg": "msg_quote"
